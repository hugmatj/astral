{
  "name": "astralapp/astral",
  "type": "project",
  "description": "Organize your GitHub stars with ease.",
  "keywords": [
    "astral",
    "github",
    "stars"
  ],
  "license": "MIT",
  "require": {
    "php": "^8.0",
    "doctrine/dbal": "^3.3",
    "fruitcake/laravel-cors": "^2.0.5",
    "genealabs/laravel-pivot-events": "^9.0",
    "genealabs/laravel-model-caching": "^0.12.0",
    "graham-campbell/github": "^10.5",
    "guzzlehttp/guzzle": "^7.2",
    "http-interop/http-factory-guzzle": "^1.0",
    "inertiajs/inertia-laravel": "^0.5.4",
    "innocenzi/laravel-vite": "^0.1.27",
    "laravel/framework": "^9.0",
    "laravel/socialite": "^5.5.1",
    "laravel/tinker": "^2.7"
  },
  "require-dev": {
    "spatie/laravel-ignition": "^1.0",
    "fakerphp/faker": "^1.9.1",
    "glhd/laravel-dumper": "^1.0.0",
    "laravel/sail": "^1.0.1",
    "mockery/mockery": "^1.4.4",
    "nunomaduro/collision": "^6.1",
    "pestphp/pest": "^1.21.1",
    "pestphp/pest-plugin-laravel": "^1.2",
    "pestphp/pest-plugin-parallel": "^1.0.0",
    "phpunit/phpunit": "^9.5.10",
    "sempro/phpunit-pretty-print": "^1.4"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-update-cmd": [
      "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ]
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
